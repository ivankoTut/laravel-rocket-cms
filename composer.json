{
    "name": "ivankotut/laravel-rocket-cms",
    "description": "The LaravelCMS Framework.",
    "keywords": ["framework", "laravel","LaravelCMS"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ivankoTut/laravel-rocket-cms"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "arrilot/laravel-widgets": "^3.7",
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.3",
        "kalnoy/nestedset": "^4.2",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelrus/sleepingowl": "4.*@dev",
        "symfony/finder": "^3.2",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Admin\\": "admin/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "php artisan sleepingowl:update"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
